{
  "name": "test1",
  "version": "1.0.0",
  "description": "API Testing on JSONPlaceholder with Jest",
  "license": "MIT",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npx jest"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "axios": "^1.8.4",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.1.0"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "outputPath": "./reports/test-report.html",
          "pageTitle": "API Testing Report - JSONPlaceholder with Jest",
          "dateFormat": "mmmm d, yyyy h:MM:ss TT",
          "includeFailureMsg": true,
          "includeConsoleLog": true
        }
      ]
    ]
  }
